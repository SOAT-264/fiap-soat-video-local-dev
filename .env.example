# Local Development Environment Variables

# Database passwords (change in production)
POSTGRES_PASSWORD=postgres123

# JWT Configuration
JWT_SECRET=dev-jwt-secret-key-change-in-production
JWT_ALGORITHM=HS256
JWT_EXPIRY_HOURS=24

# AWS LocalStack Configuration
AWS_ENDPOINT_URL=http://localhost:4566
AWS_ACCESS_KEY_ID=test
AWS_SECRET_ACCESS_KEY=test
AWS_DEFAULT_REGION=us-east-1

# S3 Buckets
S3_UPLOAD_BUCKET=video-uploads
S3_OUTPUT_BUCKET=video-outputs

# SNS Topics
SNS_VIDEO_EVENTS_TOPIC=arn:aws:sns:us-east-1:000000000000:video-events
SNS_JOB_EVENTS_TOPIC=arn:aws:sns:us-east-1:000000000000:job-events

# SQS Queues
SQS_JOB_QUEUE_URL=http://localhost:4566/000000000000/job-queue
SQS_NOTIFICATION_QUEUE_URL=http://localhost:4566/000000000000/notification-queue

# SES Configuration
SES_SENDER_EMAIL=noreply@videoprocessor.local

# Redis
REDIS_URL=redis://localhost:6379

# Service URLs (for inter-service communication)
AUTH_SERVICE_URL=http://localhost:8001
VIDEO_SERVICE_URL=http://localhost:8002
JOB_SERVICE_URL=http://localhost:8003
NOTIFICATION_SERVICE_URL=http://localhost:8004

# Log Level
LOG_LEVEL=DEBUG
